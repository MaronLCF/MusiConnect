<section>
  <!-- Start of profile-->
  <div class="feature-photo">
    <figure><img src="/cover-photo/<%=user._id%>"  alt="" /></figure>
    <!-- <div class="add-btn">
				<span>1205 followers</span>
				<a href="#" title="" data-ripple="">Add Friend</a>
			</div> -->
    <form class="edit-phto" id="edit-cover"  action="upload-cover-photo/<%=user._id%>" method ="POST"  enctype=multipart/form-data>
      <i class="fa fa-camera-retro"></i>
      <label class="fileContainer">
        Edit Cover Photo
        <input type="file" name="imgCover" id="cover-photo-upload"/>
      </label>
    </form>
    <div class="container-fluid">
      <div class="row merged">
        <div class="col-lg-2 col-sm-3">
          <div class="user-avatar">
            <figure>
              <img src="/photo/<%=user._id%>" alt="" />
              <form class="edit-phto" id="edit-phto" action="upload-photo/<%=user._id%>" method ="POST"  enctype=multipart/form-data>
                <i class="fa fa-camera-retro"></i>
                <label class="fileContainer">
                  Edit Display Photo
                  <input type="file" name ="img" id="display-photo-upload"/>
                </label>
              </form>
            </figure>
          </div>
        </div>
        <div class="col-lg-10 col-sm-9">
          <div class="timeline-info">
            <ul>
              <li class="admin-name">
                <h5> <%= user.username %>  </h5>
              </li>
              <li>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
